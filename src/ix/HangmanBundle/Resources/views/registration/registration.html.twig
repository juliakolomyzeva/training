{% extends "@Hangman/layout.html.twig" %}

{% block title 'Register - ' ~ parent() %}
{% block content %}
    <h1>Create Account</h1>
    <form action="{{ path('app_registration') }}" method="post" novalidate>
        {{ form_widget(form) }}
        <button type="submit" class="btn btn-success">Send</button>
    </form>


{% endblock %}