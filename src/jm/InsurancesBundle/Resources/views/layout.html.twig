{% block body %}
<div class="container" id="main">
    <div class="row" id="header_row">
        {% block header %}
            {{ include('@Insurances/layout/header.html.twig') }}
        {% endblock %}
    </div>
    <div class="row" id="body_row">
        <div class="container" id="body_container">
            <div class="row">
                <div class="col-md-4" id="nav_area">
                   {{ render(controller(
                    'InsurancesBundle:RelCategory:showCategoryMenu'
                    )) }}
                </div>
                <div class="col-md-8" id="content_area">
                    {% block content %}
                    {% endblock %}
                </div>
            </div>
        </div>
    </div>
    <div class="row" id="footer_row">
        {% block footer %}
            Meine Versicherungen Footer
        {% endblock %}
    </div>
</div>
