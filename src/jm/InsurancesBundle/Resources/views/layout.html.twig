{% block title 'Insurances '%}

{% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/insurances/css/bootstrap.min.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/insurances/css/insurances.css') }}" />
